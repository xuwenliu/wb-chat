/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.fl{
  float: left
}
.fr{
  float: right
}
.clear{
  clear: both;
  display: block;
}

.page-container {
  padding: 30rpx;
}

.row-container {
  display: flex;
}

.panel-container {
  margin-bottom: 100rpx;
}

.panel-container .title {
  display: block;
  margin-bottom: 40rpx;
  text-align: center;
}

.flex-between {
  display: flex;
  justify-content: space-between;
}
button{
  border-radius: 4rpx;
}
button[type="canCommit"] {
  color:#FFFFFF; 
  background: linear-gradient(269deg, #F25050 0%, #C93131 100%);
}
button[type="commitStyle2"] {
  color:#FFFFFF; 
  background:linear-gradient(269deg, #F25050 0%, #C93131 100%);
}

button[disabled=""] {
  color:#fff; 
  background-color:#ccc;
}
page{
  box-shadow: 0px 3px 5px 0 #eee inset;
}
